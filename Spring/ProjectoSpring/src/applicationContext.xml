<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.1.xsd
">

<bean id="miInforme" class="IoC.CreacionInformeImpl"></bean>

<bean id="indormePorSetter" class="IoC.SecretarioEmpleado">
    <property name="creacionInformes" ref="miInforme"></property>
    <property name="email" value="secretaria@mail.com"></property>
    <property name="nombreEmpresa" value="Casa"></property>
</bean>

<bean id="miEmpleado" class="IoC.DirectorEmpleado">
<constructor-arg ref="miInforme"></constructor-arg>
</bean>
</beans>